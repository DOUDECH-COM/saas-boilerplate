{
  "name": "@saas-boilerplate/database",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db:check": "drizzle-kit check:pg",
    "db:generate": "dotenv -e ../../.env.production drizzle-kit generate:pg",
    "db:migrate": "dotenv -e ../../.env.production ts-node ./migrate.ts",
    "db:push": "dotenv -e ../../.env.development -- drizzle-kit push",
    "db:studio": "dotenv -e ../../.env.development drizzle-kit studio"
  },
  "devDependencies": {
    "@saas-boilerplate/typescript-config": "workspace:*",
    "drizzle-kit": "^0.24.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@types/pg": "^8.11.6",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "drizzle-orm": "^0.33.0",
    "pg": "^8.12.0",
    "postgres": "^3.4.4"
  }
}
